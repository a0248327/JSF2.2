<?xml version="1.0"?>
<facelet-taglib version="2.2"
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">

    <namespace>http://at.irian/mygourmet</namespace>

    <function>
        <function-name>getAge</function-name>
        <function-class>
            at.irian.jsfatwork.mygourmet.MyGourmetUtil
        </function-class>
        <function-signature>
            int getAge(java.util.Date)
        </function-signature>
    </function>

    <tag>
        <tag-name>collapsiblePanel</tag-name>
        <component>
            <resource-id>mygourmet/collapsiblePanel.xhtml</resource-id>
        </component>
    </tag>

    <tag>
        <tag-name>dataTable</tag-name>
        <component>
            <resource-id>mygourmet/dataTable.xhtml</resource-id>
        </component>
    </tag>

    <tag>
        <tag-name>panelBox</tag-name>
        <component>
            <resource-id>mygourmet/panelBox.xhtml</resource-id>
        </component>
    </tag>

    <tag>
        <tag-name>convertZipCode</tag-name>
        <converter>
            <converter-id>at.irian.ZipCode</converter-id>
        </converter>
    </tag>

    <tag>
        <tag-name>convertList</tag-name>
        <converter>
            <converter-id>at.irian.List</converter-id>
        </converter>
    </tag>

    <tag>
        <tag-name>validateAge</tag-name>
        <validator>
            <validator-id>at.irian.Age</validator-id>
        </validator>
    </tag>

    <tag>
        <tag-name>inputSpinner</tag-name>
        <component>
            <component-type>at.irian.InputSpinner</component-type>
            <handler-class>at.irian.jsfatwork.mygourmet.InputSpinnerTagHandler</handler-class>
            <renderer-type>at.irian.InputSpinner</renderer-type>
        </component>
    </tag>

</facelet-taglib>
